	<stylesheet src="reset.css" />
	-->

	<stylesheet src="opsi-admin.css" />

	<entry-point class='cz.muni.ucn.opsi.wui.gwt.client.OpsiAdminApp' />

	<generate-with class="cz.muni.ucn.opsi.wui.gwt.rebind.beanModel.BeanModelGenerator">
		<when-type-assignable class="cz.muni.ucn.opsi.wui.gwt.client.beanModel.BeanModelLookup"/>
